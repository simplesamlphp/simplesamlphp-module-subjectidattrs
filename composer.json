{
    "name": "simplesamlphp/simplesamlphp-module-subjectidattrs",
    "description": "Module implementing SAML V2.0 Subject Identifier Attributes Profile",
    "type": "simplesamlphp-module",
    "keywords": ["simplesamlphp", "oasis", "subject"],
    "license": "LGPL-2.1-or-later",
    "authors": [
        {
            "name": "Tim van Dijen",
            "email": "tvdijen@gmail.com"
        }
    ],
    "config": {
        "preferred-install": {
            "simplesamlphp/simplesamlphp": "source",
            "*": "dist"
        },
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "simplesamlphp/composer-module-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "SimpleSAML\\Module\\subjectidattrs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimpleSAML\\Test\\Utils\\": "vendor/simplesamlphp/simplesamlphp/tests/Utils"
        }
    },
    "require": {
        "php": ">=7.4 || ^8.0",
        "simplesamlphp/assert": "~0.2.7",
        "simplesamlphp/simplesamlphp": "^2.0.0-beta.2",
        "simplesamlphp/composer-module-installer": "~1.1"
    },
    "require-dev": {
        "simplesamlphp/simplesamlphp-test-framework": "^1.1.4"
    },
    "support": {
        "issues": "https://github.com/simplesamlphp/simplesamlphp-module-subjectidattrs/issues",
        "source": "https://github.com/simplesamlphp/simplesamlphp-module-subjectidattrs"
    }
}
